<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.dsmpharm.bidding.mapper.BiddingCityMapper">


    <select id="selectAllByName" resultType="com.dsmpharm.bidding.pojo.BiddingCity">
        select * from bidding_city
        where pro_id=#{proId}
        order by city_name
    </select>
</mapper>